<h1>Edit Account</h1>

<form id="edit-account" name="account">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value="<%= name %>" >
  </div>

  <div class="field">
    <label for="bank"> bank:</label>
    <input type="text" name="bank" id="bank" value="<%= bank %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Update Account" />
  </div>

</form>

